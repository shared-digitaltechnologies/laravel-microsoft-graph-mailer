{
    "name": "shrd/laravel-microsoft-graph-mailer",
    "description": "Adds a mailer transport that sends emails using the microsoft graph API.",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "mailer",
        "azure",
        "microsoft-graph"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "Roel Hemerik",
            "email": "roel@shared.nl"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "illuminate/contracts": "^11.0",
        "symfony/mailer": "^7.0",
        "shrd/laravel-azure-identity": "^0.5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "psalm/phar": "^5.20",
        "orchestra/testbench": "^8.21"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "shrd/*": true,
            "meeva/composer-monorepo-builder-path-plugin": true
        },
        "lock": false,
        "archive-format": "zip",
        "archive-dir": "../../../dist/composer-archives/laravel-microsoft-graph-mailer",
        "bin-dir": "../../../vendor/bin",
        "vendor-dir": "../../../vendor",
        "use-parent-dir": true,
        "discard-changes": false
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shrd\\Laravel\\Azure\\MicrosoftGraphMailer\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "autoload": {
        "psr-4": {
            "Shrd\\Laravel\\Azure\\MicrosoftGraphMailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shrd\\Laravel\\Azure\\MicrosoftGraphMailer\\Tests\\": "tests/"
        }
    },
    "archive": {
        "name": "laravel-microsoft-graph-mailer",
        "exclude": [
            "/project.json",
            "/vendor",
            "tmp/",
            "*.log",
            ".phpunit.cache/",
            ".phpunit.result.cache",
            ".DS_Store",
            ".idea"
        ]
    },
    "_comment": [
        "Generated by nx-generator @shared-digitaltechnologies/nx-php:library"
    ]
}
